<!DOCTYPE html>
<html>
	<head>
	  <title>Ei Group Buying Engine</title>
	  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
	  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
	  <%= csrf_meta_tags %>
	</head>
	<body>
	<div id="header">
			<h1>EiGB Engine</h1>
		</div>
		<div id="main">
			<% if !notice.blank? %>
				<p id="notice"><%= notice %></p>
			<% end %>
			<div id="content">
				<%= yield %>
			</div>
		</div>
		<div id="footer">
			<p>&copy; 2014 Entertainment Intelligence</p>
		</div>
	</body>
</html>
